#!/usr/bin/python3
import sys
def infinite_add():
    count = 1
    sum = 0
    if len(sys.argv) == 1:
        print("0")
    else:
        for i in range(len(sys.argv)):
            sum = sum + int(sys.argv[count])
            count += 1
        print("{}".format(sum))
if __name__ == "__main__":
    infinite_add()
